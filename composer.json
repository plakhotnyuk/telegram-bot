{
    "name": "plakhotnyuk/telegram-bot",
    "description": "PHP Wrapper for Telegram Bot API",
    "type": "library",
    "keywords": ["php", "telegram", "bot", "bot api"],
    "homepage": "https://github.com/plakhotnyuk/telegram-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Plakhotniuk",
            "email": "dmitriyplakhotnyuk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "2.*",
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {
            "TelegramBot\\Api\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
